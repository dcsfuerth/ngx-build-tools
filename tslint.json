{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "no-console": [
      false
    ],
    "no-var-requires": false,
    "only-arrow-functions": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      false
    ]
  },
  "rulesDirectory": []
}
